{
  "name": "cyberdean/botdetect-bundle",
  "type": "symfony-bundle",
  "description": "Symfony CybBotDetectBundle",
  "keywords": ["security", "ban ip", "fail2ban", "bruteforce", "malicious"],
  "homepage": "https://github.com/Dean79000",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Dean79000"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cyberdean\\Security\\BotDetectBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Dean79000/CybBotDetectBunble"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "symfony/framework-bundle":     "~2.8|^3.0",
    "symfony/twig-bundle": "~2.8|^3.0",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6",
    "symfony/monolog-bundle": "^2.8"
  },
  "require-dev": {
    "sensio/generator-bundle": "^3.0",
    "symfony/phpunit-bridge": "^3.0"
  },
  "support": {
    "docs": "https://github.com/Dean79000/CybBotDetectBunble"
  }
}